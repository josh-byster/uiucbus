{
  "name": "uiucbus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@sentry/browser": "^7.18.0",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "date-fns": "^2.29.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "framer-motion": "^7.6.6",
    "lint-staged": "^13.0.3",
    "logrocket": "^3.0.1",
    "node-sass": "^8.0.0",
    "nprogress": "^0.2.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "promise-retry": "^2.0.1",
    "prop-types": "^15.8.1",
    "pulltorefreshjs": "^0.1.22",
    "react": "^18.2.0",
    "react-autosuggest": "^10.1.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-geolocated": "^4.0.3",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.1",
    "reactstrap": "^9.1.5"
  },
  "scripts": {
    "start": "WATCHPACK_POLLING=true react-scripts start",
    "lint-all": "eslint 'src/**/*.{jsx,js}'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prettier-all": "prettier --write src/**/*.js",
    "prettier-check": "prettier -c src/**/*.{js,json,css,scss,md}",
    "eject": "react-scripts eject",
    "make-cname": "cd build && echo uiucbus.com > CNAME",
    "make-404": "cp 404.html build/404.html",
    "http-server": "$(npm bin)/http-server ./build --silent -p 3000"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "http://uiucbus.com",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.{js,json,scss,md,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "cypress": "^11.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.2"
  }
}
